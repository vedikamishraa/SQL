USE SqlAssignment;

--1.Display snum,sname,city and comm of all salespeople.
SELECT Snum, Snum, City, Comm
FROM SalesPeople;
GO

--2.Display all snum without duplicates from all orders.
SELECT DISTINCT Snum
FROM Orders;

